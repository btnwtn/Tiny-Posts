<%= form_for(:sessions, url: session_path) do |form| %>
	<div>
		<% flash.each do |type, message| %>
			<h1><%= message %></h1>
		<% end %>
		<%= form.label :email %>
		<%= form.text_field :email %>
		<%= form.label :password %>
		<%= form.password_field :password %>
	</div>
	<%= form.submit "Sign In" %>
<% end %>